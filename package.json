{
  "name": "bahalu",
  "version": "0.0.1",
  "description": "Immutable container implementations to support functional programming patterns in JavaScript.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -fr dist/ *.tgz",
    "prebuild": "npm run clean && npm test",
    "build": "webpack",
    "prepublish": "npm run build",
    "pretest": "eslint src/**",
    "test": "mocha --compilers js:babel-register --check-leaks --bail --recursive src/{,**/}*_spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudaraka/bahalu.git"
  },
  "keywords": [
    "WIP",
    "container",
    "functor",
    "functional"
  ],
  "author": "Sudaraka Wijesinghe <sudaraka@sudaraka.org> (https://sudaraka.org/)",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/sudaraka/bahalu/issues"
  },
  "homepage": "https://github.com/sudaraka/bahalu#readme",
  "devDependencies": {
    "@sudaraka/eslint-config-default": "^0.6.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^3.2.0",
    "expect": "^1.14.0",
    "mocha": "^2.4.5",
    "webpack": "^1.13.1"
  }
}
